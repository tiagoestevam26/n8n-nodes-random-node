{
  "name": "n8n-nodes-random-node",
  "version": "1.0.0",
  "description": "Custom node for generating random numbers for n8n",
  "main": "dist/nodes/Random/Random.node.js",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "random",
    "generator"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Tiago",
    "email": "tiago@onfly.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoestevam26/n8n-nodes-random-node.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "dev": "tsc --watch",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f n8n",
    "setup": "node scripts/setup.js",
    "start": "npm run docker:build && npm run docker:up",
    "stop": "npm run docker:down",
    "restart": "npm run docker:down && npm run start"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "n8n-workflow": "latest"
  }
}
